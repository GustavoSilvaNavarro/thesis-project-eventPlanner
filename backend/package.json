{
  "name": "server-api-for-event-planning-app",
  "version": "1.0.0",
  "description": "API for Event Planning App",
  "main": "dist/app.js",
  "scripts": {
    "start": "node .",
    "prestart": "npm run build:webpack",
    "format-check:prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "start:dev": "nodemon src/app.ts --exec ts-node",
    "build": "tsc",
    "build:webpack": "webpack",
    "start:production": "npm run build:webpack && node .",
    "clean": "rmdir dist",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "Typescript",
    "Express",
    "SQL",
    "REST API",
    "SEQUELIZE",
    "POSTGRES",
    "JEST",
    "SOCKET.IO",
    "CLOUDINARY",
    "MULTER"
  ],
  "author": "Gustavo Silva<gustavo.silva.nav@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "cloudinary": "^1.32.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs-extra": "^10.1.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.8",
    "socket.io": "^4.5.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.4",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "supertest": "^6.3.2",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
